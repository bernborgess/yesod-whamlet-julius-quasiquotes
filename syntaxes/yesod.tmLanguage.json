{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "yesod - Quasi Html JS",
	"patterns": [
		{
			"include": "#julius"
		},
		{
			"include": "#whamlet"
		},
		{
			"include": "#haskell"
		}
	],
	"repository": {
		"julius": {
			"name": "source.js.yesod",
			"begin": "\\[julius\\|",
			"end": "\\|\\]",
			"patterns": [
				{
					"include": "source.js"
				}
			]
		},
		"whamlet": {
			"name": "text.html.basic.yesod",
			"begin": "\\[whamlet\\|",
			"end": "\\|\\]",
			"patterns": [
				{
					"include": "text.html.basic"
				}
			]
		}
	},
	"scopeName": "source.yesod"
}