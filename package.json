{
  "name": "yesod-whamlet-julius-quasiquotes",
  "displayName": "yesod-whamlet-julius-quasiquotes",
  "description": "It's a support for different syntax highlighting inside quasiquotes in yesod",
  "publisher": "bernborgess",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "author": {
    "name": "Bernardo Borges",
    "email": "bernborgess@outlook.com",
    "url": "https://www.linkedin.com/in/bernborgess/"
  },
  "repository": {
    "url": "https://github.com/bernborgess/yesod-whamlet-julius-quasiquotes",
    "type": "git"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "yesod",
        "aliases": [
          "yesod - Quasi Html JS",
          "yesod"
        ],
        "extensions": [
          ".hs"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "yesod",
        "scopeName": "source.yesod",
        "path": "./syntaxes/yesod.tmLanguage.json"
      }
    ]
  }
}